                                    <span>Tel</span>
                                    <span><a href="tel:+33160766404">01 60 76 64 04</a></span>
                                </div>
                            </div>
                            <div class="col-md topper d-flex align-items-center justify-content-end">
                                <p class="mb-0 d-block">
                                    <a href={%  if app.user is not null and is_granted('ROLE_ADMIN') %}"{{ url('blog_admin') }}" {% else %} "#" {% endif %} class="btn py-2 px-3 btn-primary">
                                        <span>{%  if app.user is not null and is_granted('ROLE_ADMIN') %} Administration {% else %} Contactez-nous {% endif %}</span>
                                    </a>
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>


